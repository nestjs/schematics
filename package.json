{
  "name": "@nestjs/schematics",
  "version": "5.3.2",
  "description": "Nestjs schematics project for @nestjs/cli",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint:src": "tslint --project tsconfig.json",
    "lint:test": "tslint --project test/tsconfig.json",
    "test": "nyc mocha test/**/*.ts --require ts-node/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nestjs/schematics.git"
  },
  "author": "ThomRick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nestjs/schematics/issues"
  },
  "homepage": "https://github.com/nestjs/schematics#readme",
  "dependencies": {
    "@angular-devkit/core": "^0.4.8",
    "@angular-devkit/schematics": "^0.4.8",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "ts-lint": "^4.5.1",
    "ts-node": "^4.1.0"
  },
  "schematics": "./collection.json",
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules",
      "test"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
